{
  "id": "io.github.ireaderorg.plugins.tachi-loader",
  "name": "Tachiyomi Source Loader",
  "version": "1.0.0",
  "versionCode": 1,
  "description": "Load Tachiyomi/Mihon manga extensions in IReader. Supports browsing, searching, and reading manga from hundreds of sources.",
  "author": {
    "name": "IReader Team"
  },
  "type": "TACHI_SOURCE_LOADER",
  "permissions": ["NETWORK", "STORAGE", "PREFERENCES"],
  "minIReaderVersion": "1.0.0",
  "platforms": ["ANDROID", "DESKTOP"],
  "mainClass": "io.github.ireaderorg.plugins.tachiloader.TachiLoaderPlugin",
  "metadata": {
    "tachi.supportedLibVersionMin": "1.3",
    "tachi.supportedLibVersionMax": "1.5",
    "tachi.defaultRepo": "https://raw.githubusercontent.com/keiyoushi/extensions/repo"
  }
}
